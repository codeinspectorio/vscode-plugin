  "scripts": {
    "start": "REACT_APP_CI_ENV=devel REACT_APP_REVISION=`git show HEAD^1 | grep commit | head | awk '{print $2}'` node scripts/start.js",
    "build-staging": "REACT_APP_CI_ENV=staging REACT_APP_REVISION=`git show HEAD^1 | grep commit | head | awk '{print $2}'` node scripts/build.js",
    "build-prod": "REACT_APP_CI_ENV=prod REACT_APP_REVISION=`git show HEAD^1 | grep commit | head | awk '{print $2}'` node scripts/build.js",
    "test": "node scripts/test.js --env=jest-environment-jsdom-sixteen",
    "deploy": "aws s3 cp --recursive ./build s3://frontend-beta"
